+  nm = "nm"
+  ar = "ar"
+  ld = cxx
+
+  toolchain_cpu = "x86"
+  toolchain_os = "openbsd"
+  is_clang = false
+}
+
+clang_toolchain("clang_x64") {
+  toolchain_cpu = "x64"
+  toolchain_os = "openbsd"
+}
+
