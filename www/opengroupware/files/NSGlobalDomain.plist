    <string>YES</string>
    <key>WOMessageUseUTF8</key>
    <string>YES</string>
    <key>WOParsersUseUTF8</key>
    <string>YES</string>
    <key>imap_host</key>
    <string>127.0.0.1</string>
    <key>tar</key>
    <string>${TRUEPREFIX}/bin/gtar</string>
    <key>unzip</key>
    <string>${TRUEPREFIX}/bin/unzip</string>
    <key>zip</key>
    <string>${TRUEPREFIX}/bin/zip</string>
    <key>zipinfo</key>
    <string>${TRUEPREFIX}/bin/zipinfo</string>
