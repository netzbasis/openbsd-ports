    <string>YES</string>
    <key>EnableSieveFilter</key>
    <string>YES</string>
    <key>LDAPInitialBindSpecific</key>
    <string>YES</string>
    <key>LDAPLoginAttributeName</key>
    <string>uid</string>
    <key>LSAttachmentPath</key>
    <string>/var/opengroupware/documents</string>
