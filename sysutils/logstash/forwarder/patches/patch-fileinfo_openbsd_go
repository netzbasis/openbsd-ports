--- fileinfo_openbsd.go.orig	Tue Dec 16 08:46:53 2014
+++ fileinfo_openbsd.go	Tue Dec 16 08:47:20 2014
@@ -0,0 +1,11 @@
+package main
+
+import (
+  "os"
+  "syscall"
+)
+
+func file_ids(info *os.FileInfo) (uint64, int32) {
+  fstat := (*(info)).Sys().(*syscall.Stat_t)
+  return fstat.Ino, fstat.Dev
+}
