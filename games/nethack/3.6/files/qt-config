VARDATND = $(XFILES)
CFLAGS+=-DQT_GRAPHICS -I$(MODQT_INCDIR) -pthread
LFLAGS =-pthread -L$(MODQT_LIBDIR) -L$(X11BASE)/lib -L$(LOCALBASE)/lib

WINSRC = $(WINTTYSRC) $(WINQTSRC)
WINOBJ = $(WINTTYOBJ) $(WINQTOBJ)
WINLIB = $(WINTTYLIB) -lqt-mt -lSM
QTDIR  = $(LOCALBASE)
LINKCMD = c++
