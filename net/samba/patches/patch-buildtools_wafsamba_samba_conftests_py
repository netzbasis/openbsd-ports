-                 env.CPPPATH = [os.path.join(x, 'include')] + env.CPPPATH
+                 env.CPPPATH = env.CPPPATH + [os.path.join(x, 'include')]
              if not x in env.LIBPATH:
-                 env.LIBPATH = [os.path.join(x, 'lib')] + env.LIBPATH
+                 env.LIBPATH = env.LIBPATH + [os.path.join(x, 'lib')]
 
